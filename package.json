{
  "name": "capacitor-background-fetch",
  "version": "0.0.4",
  "description": "Utilizing the iOS background fetch mechanism to fetch new data while the app is in background",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "kaunstdadenga",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "1.0.0-beta.22"
  },
  "devDependencies": {
    "typescript": "^3.2.4",
    "@capacitor/ios": "1.0.0-beta.22",
    "@capacitor/android": "latest"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorBackgroundFetch.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaunstdadenga/capacitor-background-fetch.git"
  },
  "bugs": {
    "url": "https://github.com/kaunstdadenga/capacitor-background-fetch.git/issues"
  }
}
